{
    "devCommand": "uvicorn src.main:app --host 0.0.0.0 --port 8080 --timeout-keep-alive 120",
    "builds": [
      {
        "src": "src/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.py"
      }
    ],
    "functions": {
      "app/api/**/*": {
        "maxDuration": 100
      }
    }
  }
