{
    "devCommand": "uvicorn api.main:app --host 0.0.0.0 --port 8080 --timeout-keep-alive 120",
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/main.py"
      }
    ],
    "functions": {
      "api/main.py": {
        "maxDuration": 100
      }
    }
  }
