{
    "devCommand": "uvicorn src.main:app --host 0.0.0.0 --port 8080",
    "builds": [
      {
        "src": "src/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.py"
      }
    ],
    "rewrites": [
      { "source": "/src", "destination": "/api" }
    ],
    "functions": {
      "api/main.py": {
        "maxDuration": 100
      }
    }
  }
